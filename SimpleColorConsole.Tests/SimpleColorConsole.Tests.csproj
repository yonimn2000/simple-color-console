    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\SimpleColorConsole\SimpleColorConsole.csproj" />
  </ItemGroup>

</Project>
